{
  "name": "balancer",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create balancer-labs/balancer-v2 --node https://api.thegraph.com/deploy/",
    "remove:local": "graph remove embrfinance/embr-fuji-v2 --node http://127.0.0.1:8020",
    "create:local": "graph create embrfinance/embr-fuji-v2 --node http://127.0.0.1:8020",
    "deploy:local": "graph deploy embrfinance/embr-fuji-v2 subgraph.yaml --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
    "codegen": "graph codegen --output-dir src/types/",
    "auth": "graph auth",
    "build": "graph build",
    "deploy:avalanche": "graph deploy embrfinance/embr-avalanche-v2 subgraph.avalanche.yaml",
    "deploy:velas": "graph deploy --product hosted-service --node http://127.0.0.1:8020 --ipfs http://127.0.0.1:5001 wavelength22 subgraph.yaml",
    "remove:velas": "graph remove --product hosted-service --node http://127.0.0.1:8020 --ipfs http://127.0.0.1:5001 masterchef subgraph.yaml",
    "lint": "eslint --max-warnings 0 . --ext .ts",
    "test": "jest ./test/*.test.ts",
    "deploy": "graph deploy balancer-labs/balancer-v2 --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy:kovan": "graph deploy balancer-labs/balancer-kovan-v2 subgraph.kovan.yaml --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy:fuji": "graph deploy beethovenx subgraph.fuji.yaml",
    "deploy:fantom-eu": "graph deploy beethovenx-v4 subgraph.fantom.yaml --ipfs  http://ec2-3-126-251-111.eu-central-1.compute.amazonaws.com:5001 --node  http://ec2-3-126-251-111.eu-central-1.compute.amazonaws.com:8020/",
    "deploy:fantom-us": "graph deploy beethovenx-v4 subgraph.fantom.yaml --ipfs  http://ec2-54-219-138-209.us-west-1.compute.amazonaws.com:5001 --node  http://ec2-54-219-138-209.us-west-1.compute.amazonaws.com:8020/",
    "deploy:fantom-canada": "graph deploy beethovenx-v4 subgraph.yaml --ipfs  http://ec2-99-79-76-55.ca-central-1.compute.amazonaws.com:5001 --node  http://ec2-99-79-76-55.ca-central-1.compute.amazonaws.com:8020/",
    "deploy:fantom-seoul": "graph deploy beethovenx-v4 subgraph.fantom.yaml --ipfs http://ec2-13-125-206-95.ap-northeast-2.compute.amazonaws.com:5001/ --node http://ec2-13-125-206-95.ap-northeast-2.compute.amazonaws.com:8020/",
    "deploy:rinkeby": "graph deploy beethovenx-rinkeby subgraph.rinkeby.yaml",
    "import-abis": "ts-node ./scripts/update-abis.ts",
    "generate-manifests": "ts-node ./scripts/generate-manifests",
    "update-addresses": "ts-node ./scripts/update-addresses-subgraph.ts",
    "update-addresses:kovan": "yarn update-addresses kovan",
    "update-addresses:ropsten": "yarn update-addresses ropsten",
    "update-addresses:rinkeby": "yarn update-addresses rinkeby",
    "update-addresses:goerli": "yarn update-addresses goerli",
    "update-addresses:local": "yarn update-addresses dockerParity",
    "sync": "yarn import-abis && yarn update-addresses && yarn codegen && yarn build",
    "sync:kovan": "yarn import-abis && yarn update-addresses kovan && yarn codegen && yarn build",
    "sync:local": "yarn import-abis && yarn update-addresses dockerParity && yarn codegen && yarn build",
    "deploy:testgraph": "graph deploy testgraph subgraph.fantom.yaml --ipfs http://localhost:5001/ --node http://localhost:8020/"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.9",
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@graphprotocol/graph-cli": "^0.25.0",
    "@graphprotocol/graph-ts": "^0.25.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.4.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "apollo-client": "^2.6.10",
    "assemblyscript": "0.19.10",
    "babel-jest": "^26.6.3",
    "eslint": "^7.9.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.7",
    "jest": "^26.6.3",
    "js-yaml": "^4.0.0",
    "path": "^0.12.7",
    "prettier": "^2.1.2",
    "ts-jest": "^26.5.0",
    "ts-node": "^10.1.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.17.5",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "global": "^4.4.0",
    "node-fetch": "^2.6.1"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "preset": "ts-jest"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        },
        "@babel/preset-typescript"
      ]
    ]
  }
}
